<div class="container">
  <p id ="notice"></p>
    <h4 class="text-dark bg-light w-25 text-center mb-y">会員一覧</h4>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">会員ID</th>
          <th scope="col">氏名</th>
          <th scope="col">メールアドレス</th>
          <th scope="col">ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%#= link_to  admin_customer_path(customer.id)  do %>
            <%= user.name %>
            <%# end %>
          </td>
          <td><%= user.email  %></td>
          <td><% if user.is_deleted == false %>
              有効
              <% else %>
              退会
              <% end %>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
</div>